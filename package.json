{
  "name": "intro-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "dev": "NODE_ENV=dev nodemon dist/server.js ",
    "serve": "node dist/server.js",
    "build": "npm run tslint && npm run build-ts",
    "build-ts": "tsc",
    "watch": "npm run tslint && npm run watch-ts ",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^4.0.3",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-swagger-decorator": "^1.7.8",
    "koa-views": "^6.2.0",
    "koa2-cors": "^2.0.6",
    "mongoose": "^6.0.11",
    "pug": "^2.0.3"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "nodemon": "^1.19.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  }
}
